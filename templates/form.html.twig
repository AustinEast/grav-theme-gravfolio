{% extends 'partials/base.html.twig' %}

{% block content %}
{% if page.header.headerToggle %}
    <header>
	   <div>
           {{ page.content }}
        </div>
    </header>
    <section id="contact">
    {% else %}
    <section id="contact">
        <h2>{{ page.title }}</h2>
        {{ page.content }}
    {% endif %}
        {% include "forms/form.html.twig" %}
    </section>
{% endblock %}
        
{% block bottom %}
    {% do assets.add('http://code.jquery.com/jquery-1.11.0.min.js', 101) %}
    {% do assets.add('http://cdn.jsdelivr.net/jquery.mixitup/latest/jquery.mixitup.min.js', 100) %}
    {% do assets.add('theme://js/showup.js', 99) %}
    {% do assets.add('theme://js/functions.js', 98) %} 
    {{ assets.js()}}
{% endblock %}
